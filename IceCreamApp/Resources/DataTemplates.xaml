<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:store="clr-namespace:IceCreamApp.ViewModels.Store"
                    xmlns:store1="clr-namespace:IceCreamApp.Views.Store"
                    xmlns:iceCream="clr-namespace:IceCreamApp.ViewModels.IceCream"
                    xmlns:iceCream1="clr-namespace:IceCreamApp.Views.IceCream">

    <DataTemplate DataType="{x:Type store:StoreListViewModel}">
        <store1:StoreListView></store1:StoreListView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type store:StoreEditViewModel}">
        <store1:StoreEditView></store1:StoreEditView>
    </DataTemplate>


    <DataTemplate DataType="{x:Type iceCream:IceCreamListViewModel}">
        <iceCream1:IceCreamListView></iceCream1:IceCreamListView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type iceCream:IceCreamGridViewModel}">
        <iceCream1:IceCreamGridView></iceCream1:IceCreamGridView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type iceCream:IceCreamDetailViewModel}">
        <iceCream1:IceCreamDetailView></iceCream1:IceCreamDetailView>
    </DataTemplate>


    
</ResourceDictionary>